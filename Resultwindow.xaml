<Window x:Class="XbimFederationChecker.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XbimFederationChecker"
        mc:Ignorable="d"
        Title="Results" Height="768" Width="1200">
    <Grid>
        <TabControl Name="tabControl1">
            <TabItem Header="Data">
                <DataGrid x:Name="dataGrid">
                </DataGrid>
            </TabItem>
            <TabItem Header="XY View" >
                <Canvas Name="XYcanvas">
                </Canvas>
            </TabItem>
            <TabItem Header="YZ View" >
                <Canvas Name="YZcanvas">
                </Canvas>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
